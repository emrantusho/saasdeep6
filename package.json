{
  "name": "saasdeep6",
  "version": "1.0.0",
  "description": "A browser-like showcase portfolio managed by Sveltia CMS.",
  "scripts": {
    "start": "concurrently \"npm:watch:css\" \"npm:serve\"",
    "build": "npm run build:css && eleventy",
    "serve": "eleventy --serve",
    "watch:css": "tailwindcss -i ./public/css/style.css -o ./_site/css/style.css --watch",
    "build:css": "tailwindcss -i ./public/css/style.css -o ./_site/css/style.css --minify"
  },
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1", "autoprefixer": "^10.4.16", "concurrently": "^8.2.2",
    "postcss": "^8.4.31", "tailwindcss": "^3.3.5"
  }
}
